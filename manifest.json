{
   "manifest_version": 2,

   "name": "Gravatar Ident",
   "description": "Replace blank Gravatar with Gravatar Identicons",
   "version": "0.1",
   "icons": {
        "256": "icon.png"
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "http://*/*", "https://*/*"],
      "run_at": "document_idle"
   } ]
}
